<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="user" namespace="/" extends="default">
		<action name="user*" class="org.icer.ztssh.action.UserAction" method="{1}">
			<result>${nextpage }</result>
			<result name="success_red" type="redirect">${nextpage }</result>
			<result name="success_blue" type="redirect">${nextpage }</result>
			<result name="success_yellow" type="redirect">${nextpage }</result>
		</action>
	</package>
		
</struts>
